<!-- Dopplerlog RAW stream description -->
<!-- $Id$ -->

<stream name="dopplerlog">
	<delim>,</delim>

	<vars field="1" name="-velocity_f_a"              units="knots" type="string" />
	<vars field="2" name="-velocity_p_s"              units="knots" type="string" />
	<vars field="3" name="-water_speed_status"        units=" "     type="string" />
	<vars field="4" name="-ground_speed_longitudinal" units="knots" type="string" />
	<vars field="5" name="-ground_speed_transverse"   units="knots" type="string" />
	<vars field="6" name="-ground_speed_status"       units=" "     type="string" />
</stream>
