<!-- Ashtech RAW stream description -->
<!-- $Id: ashtech.xml 527 2009-10-01 16:38:24Z jpro $ -->

<stream>
	<delim>,</delim>

	<!-- field starts at 0 after RAW timestamp -->
	<!-- field names beginning with - are prefixed with stream name -->
	<vars field="1"  name="-time"	  units="HHMMSS.SS" type="string" />
  	<vars field="2"  name="-lat"      units="degrees"   type="latlon" />
  	<vars field="4"  name="-lon"      units="degrees"   type="latlon" />
  	<vars field="6"  name="-altitude" units="m"         type="string" />
  	<vars field="7"  name="-heading"  units="degrees"   type="string" />
  	<vars field="8"  name="-pitch"    units="degrees"   type="string" />
  	<vars field="9"  name="-roll"     units="degrees"   type="string" />
  	<vars field="10" name="-mrms"     units="m"         type="string" />
  	<vars field="11" name="-brms"     units="m"         type="string" />
  	<vars field="12" name="-aq"       units=" "         type="prechecksum_string" />
</stream>
