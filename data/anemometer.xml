<!-- Anemometer RAW stream description -->
<!-- $Id: anemometer.xml 527 2009-10-01 16:38:24Z jpro $ -->

<stream>
	<delim>,</delim>

	<!-- field starts at 0 after RAW timestamp -->
	<!-- field names beginning with - are prefixed with stream name -->
	<vars field="1" name="-wind_dir"   units="degrees" type="string" />
	<vars field="3" name="-wind_speed" units="knots"   type="string" />
</stream>
