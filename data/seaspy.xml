<!-- SeaSPY RAW stream description -->
<!-- $Id$ -->

<stream>
	<delim>,</delim>

	<!-- Only try and convert files whose raw string is prefixed with this -->
	<data_prefix>$</data_prefix>

	<!-- SCS include '$ ' at start of magnetic field -->
	<vars field="0"  name="-magnetic_field"  units="nT" spos="2" epos="10" type="string" />
	<vars field="1"  name="-signal_strength" units=" "  type="string" />
	<vars field="2"  name="-depth"           units="m"  spos="17" epos="20" type="string" />
</stream>
