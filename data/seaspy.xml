<!-- SeaSPY RAW stream description -->
<!-- $Id: seaspy.xml 528 2009-10-01 17:15:22Z jpro $ -->

<stream>
	<delim>,</delim>

	<!-- SCS include '$ ' at start of magnetic field -->
	<vars field="1"  name="-magnetic_field"  units="nT" spos="2" epos="10" type="string" />
	<vars field="2"  name="-signal_strength" units=" "  type="string" />
	<vars field="3"  name="-depth"           units="m"  spos="0" epos="3" type="string" />
</stream>
