<!-- Netmonitor RAW stream description -->
<!-- $Id: netmonitor.xml 528 2009-10-01 17:15:22Z jpro $ -->

<stream>
	<delim>,</delim>

	<vars field="1"  name="-flow"     units="msec"    type="string" />
	<vars field="2"  name="-depth"    units="m"       type="string" />
	<vars field="3"  name="-pitch"    units="degrees" type="string" />
	<vars field="4"  name="-roll"     units="degrees" type="string" />
	<vars field="5"  name="-cond"     units=" "       type="string" />
	<vars field="6"  name="-spare"    units=" "       type="string" />
	<vars field="7"  name="-par"      units="mV"      type="string" />
	<vars field="8"  name="-fluor"    units=" "       type="string" />
	<vars field="9"  name="-altitude" units="m"       type="string" />
	<vars field="10" name="-temp"     units="celsius" type="string" />
	<vars field="11" name="-mot0"     units=" "       type="string" />
	<vars field="12" name="-mot1"     units=" "       type="string" />
	<vars field="13" name="-cmd"      units=" "       type="string" />
</stream>
